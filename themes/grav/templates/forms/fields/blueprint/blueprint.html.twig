{% extends "forms/field.html.twig" %}

{% block field %}
<input
    data-grav-field="hidden"
    data-grav-disabled="false"
    type="hidden"
    class="input"
    {% if field.disabled or isDisabledToggleable %}disabled="disabled"{% endif %}
    name="{{ (scope ~ field.name)|fieldName }}"
    value="{{ blueprints.name }}" />
{% endblock %}